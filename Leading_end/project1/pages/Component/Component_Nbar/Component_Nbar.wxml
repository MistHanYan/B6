<!--pages/Component/Component_Nbar/Component_Nbar.wxml-->
<view class="Nbar">
<!-- 跳转到识别页面 -->
  <view class="Nbar-child" bindtap="bindViewTap">
    <image src="../img/搜索.png" mode="" style="width: 100%; height: 100rpx;" />
    <text>识别</text>
  </view>
  <!-- 跳转到收藏页面 -->
  <view class="Nbar-child" bindtap="bindViewTapcollect">
    <image src="../img/收藏.png" mode="" style="width: 100%; height: 100rpx;" />
    <text>收藏</text>
  </view>
  <!-- 跳转到用户页面 -->
  <view class="Nbar-child" bindtap="bindViewTapuser">
    <image src="../img/用户.png" mode="" style="width: 100%; height: 100rpx;" />
    <text>用户</text>
  </view>
</view>