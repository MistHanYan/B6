/* pages/recognintion/recognition.wxss */
.shibie{
  height: 85vh;
  display: flex;
  /* justify-content :center; */
  align-items:center;
  flex-direction:column;
  background-color: rgb(239, 239, 239);
}
.identify{
  overflow: auto;
  width: 100%;
  height: 500rpx;
  display: flex;
  justify-content :center;
  align-items:center;
  flex-direction:column;
  background-color: black;
}

.identify_img{
  width: 100%;
  height: 100%;
}
.open-and-identify{
  width: 50%;
  height: 7vh;
  margin: 10rpx;
  background-color: chartreuse;
  border-radius: 9px;
  display: flex;
  justify-content :center;
  align-items:center;
  flex-direction:column;
}

.Display-Results{
  width: 70%;
  height: 7vh;
  margin: 20rpx;
  padding: 0 50rpx;
  background-color: chartreuse;
  display: flex;
  justify-content :space-between;
  align-items:center;
}

.join-and-return{
  width: 70%;
  height: auto;
  margin: 20rpx;
  padding: 0 50rpx;
  /* background-color: chartreuse; */
  display: flex;
  /* justify-content :space-between; */
  align-items:center;
  flex-direction:column
}

.join{
  width: 100%;
  height: 10vh;
  text-align: center;
  border-radius: 9px;
  background-color: cornflowerblue;
  display: flex;
  flex-direction:column;
  align-items:center;
  justify-content:center;
  margin-bottom: 10px;
}